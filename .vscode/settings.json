{
  "eslint.workingDirectories": ["frontend"],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "eslint.options": {
    "extensions": [".js", ".jsx"]
  },
  "files.associations": {
    "*.test.js": "javascript",
    "*.test.jsx": "javascriptreact",
    "*.spec.js": "javascript",
    "*.spec.jsx": "javascriptreact"
  }
}